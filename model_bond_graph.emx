    type 'WaveGenerator-VariableTimeSquare'
     ports
      signal out output;
    end;
    icon bg bottom
     figures
      rectangle 128 848 160 880 color 0 fill 15132390;
      line 131.8 852.5 131.9 878.2 color 0 fill 0;
      line 129.9 876.1 155.9 876.1 color 0 fill 0;
      line 131.3 876.1 138 876 138 864 color 16711680 fill 0;
      line 142.7 876.1 142.7 864 142.7 876.1 color 16711680 fill 0;
      line 152 864 152 876.1 152 864 color 16711680 fill 0;
      line 156.2 864.1 152 864.1 color 16711680 fill 0;
      line 143.5 876.2 151.5 876.2 color 16711680 fill 0;
      line 156.2 876.4 156.2 864.3 156.2 876.4 color 16711680 fill 0;
      line 142.4 864.2 138.2 864.2 color 16711680 fill 0;
    end;
    implementation eq
parameters
	real interval = 1.0 {s};
	real pulselength = 0.1 {s};
	real amplitude = 1.0;			// amplitude of the pulse
variables
	real hidden x;						// x changes between 1 and 0
	real hidden nextTime;
	boolean hidden timetrigger;
initialequations
	if pulselength >= interval then stopsimulation ('pulse length should be smaller than interval'); end;
	x = 0.0;
	nextTime = time + interval - pulselength;
code
	timetrigger = timeevent(nextTime);

	if( timetrigger ) then
		if x == 0.0 then
			// give the negated value of x to the output
			x = 1.0;
			// and determine the next time
			nextTime = time + pulselength;
		else
			// give the negated value of x to the output
			x = 0.0;
			// and determine the next time
			nextTime = time + interval - pulselength;
		end;
	end;

	// and calculate the output
	output = amplitude * x;
implementation_end;

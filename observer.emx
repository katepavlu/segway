       type 'Filter-LowPass1'
        ports
         signal in u;
         signal out y;
        parameters
         real K = 1;
         real f = 10 {Hz};
       end;
       icon bg bottom
        figures
         rectangle 271 9 321 39 color 0 fill 15132390;
         text 'LP-1' 296 19 color 16711680 14 bold;
         text 'filter' 296 31 color 0;
       end;
       implementation eq
// 20-sim Filter Editor
// 'Low Pass First Order Filter' 1 10
variables
	real hidden n[2], d[2];
	real hidden s[1];
initialequations
	// create numerator and denominator coefficients
	n = [0; K];
	d = [1 / (2 * pi * f); 1];
equations
	// the actual dynamic equations
	s = int ( [
		n[2] * u - d[2] * y
	] );
	y = (1/d[1]) * (n[1] * u + s[1]);
implementation_end;

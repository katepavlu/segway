         signal boolean in reset;
         signal out relative_output;
       end;
       icon bg bottom
        figures
         rectangle 64.1 264 95.9 296 color 0 fill 15132390;
         text 'K' 80 280 color 16711680 16 bold;
       end;
       implementation eq
//Converts a wrapped angle signal to a unwrapped signal
//EXPECTS: 
// input: 0 to 1 signal representing rotations
// reset: Sets the current postition to the offset value.
//OUTPUT: 
//	output: Angle singal of the system.
// relative_output: Angle signal uncorrected by resets.

parameters
	real offset = 0; //On what value the system should reset after a reset is applied.
variables 
	real rotations; // How many rotaions of the wheel have happened
	real hidden delayed;
	boolean hidden delayed_reset;
	real initial; // The apparant initial position after a reset
initialequations
	initial = 0;
	rotations = 0;
code

delayed = previous(input,0);
if (input - delayed >  0.5) then
			rotations = rotations-1;
	else
	if (input - delayed <  -0.5) then
			rotations = rotations+1;
	else
		rotations = rotations;
	end;
end;

delayed_reset = previous(reset,false);
if (reset and not delayed_reset) then
	initial = (offset+ (rotations + input)) ; 
end;
output = rotations + input - initial;
relative_output = rotations + input;
    implementation_end;
